  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es5",
    "module": "es6",
    "downlevelIteration": true,
    "baseUrl": "../",
    "types": ["node"]
  },
  "include": [
    "../esm/utils/*",
    "../esm/components/*",
    "../esm/index.ts"
  ],
  "exclude": [
    "../node_modules/*"
  ]
}
